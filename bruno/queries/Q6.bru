meta {
  name: Query 6
  type: graphql
  seq: 6
}

post {
  url: {{baseUrl}}/graphql
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:graphql {
  query {
  praticien(filter: { structure_id: { ville: { _eq: "Paris" } } }) {
    id
    nom
    prenom
    ville
    structure_id {
      nom
      ville
    }
  }
}
}
